{
  "name": "oreum",
  "private": true,
  "version": "1.0.0",
  "license": "ISC",
  "scripts": {
    "start": "node ./server-dist/index.js",
    "watch-start": "nodemon --watch ./server-dist ./server-dist/index.js",
    "build": "npx tsc -p ./server",
    "watch-build": "npx tsc -p ./server --watch",
    "app:build": "vue-tsc --noEmit -p ./app && vite build -c ./app/vite.config.ts",
    "app:watch-build": "vue-tsc --noEmit -p ./app && vite build -c ./app/vite.config.ts --watch",
    "app:preview": "vite preview -c ./app/vite.config.ts",
    "app:dev": "vite -c ./app/vite.config.ts"
  },
  "dependencies": {
    "@types/eslint": "^8.4.9",
    "@types/mime-types": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.26.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-vue": "^9.7.0",
    "express": "^4.18.2",
    "mime-types": "^2.1.35",
    "nodemon": "^2.0.20",
    "pinia": "^2.0.23",
    "sass": "^1.55.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.0",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9"
  }
}
