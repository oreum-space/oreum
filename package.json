    "app:watch-build": "vue-tsc --noEmit -p ./app && vite build -c ./app/vite.config.ts --watch",
    "app:preview": "vite preview -c ./app/vite.config.ts",
    "app:build": "vue-tsc --noEmit -p ./app && vite build -c ./app/vite.config.ts",
    "server:update": "powershell ./update.ps1"
